<script type="text/javascript">
$(function(){
    $('.t-backtop').click(function () {
        $('body,html').animate({
            scrollTop: 0
        }, 800);
        return false;
    });

    $(".jumper").on("click", function( e ) {
        e.preventDefault();

        $("body, html").animate({ 
            scrollTop: $( $(this).attr('href') ).offset().top 
        }, 900);
    });

    $('.t-backtop').hide();
    
    var $win = $(window);
     $win.scroll(function () {
         if ($win.scrollTop() == 0){
            $('.t-backtop').hide();
            $('header.head').removeClass('scrollview');
         }else if ($win.height() + $win.scrollTop() != $(document).height() || $win.height() + $win.scrollTop() > 400) {
            $('.t-backtop').show();
            $('header.head').addClass('scrollview');
         }
     });
});
</script>


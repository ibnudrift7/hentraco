{% extends layout %}
{% block title %}Machine - {% endblock %}
{% block menu %}machine_detail{% endblock %}
{% block content %}

<section class="cover-insidepage prelatife">
    {% include "/layouts/header.twig" with { 'menu': block('menu') } %}

    <div class="pictures">
        <img src="{{ app.request.baseurl }}/asset/images/ill-machine-detail.jpg" alt="" class="img img-fluid">
    </div>
    <div class="nblocks_desc text-center">
        <div class="inner">
        <h1>Our Machines</h1>
        </div>
    </div>
</section>


<section class="insides_page pg_default pg-machine" >
    <div class="prelatife container_def">
        <div class="content-texts inners text-center">
            <div class="py-5"></div>
            
            <div class="text-center tops_titledetail">
                <h1>Lathe Machine</h1>
            </div>
            <div class="tops_filter">
                <div class="row">
                    <div class="col-md-20">
                        <form class="form-inline">
                          <label for="inlineForm">See other category</label>
                          <select name="categorys" id="">
                              <option value="">Lathe</option>
                              <option value="">Lathe 1</option>
                              <option value="">Lathe 2</option>
                          </select>
                        </form>
                    </div>
                    <div class="col-md-20">
                        <p class="m-0">Our Machine Tool & Industrial Machinery</p>
                    </div>
                    <div class="col-md-20 text-right">
                        <a href="#" class="btn btn-link p-0 backs_to">Back to product category</a>
                    </div>
                </div>
            </div>
            <div class="py-3 my-4"></div>

            <div class="lists_detail_products">

                <div class="items">
                    <div class="row no-gutters">
                        <div class="col-md-30">
                            <div class="pictures back-white">
                                <img src="{{ app.request.baseurl }}/asset/images/machine_n1_big.jpg" alt="" class="img img-fluid d-block mx-auto">
                            </div>
                        </div>
                        <div class="col-md-30 back-grey my-auto text-left p-4">
                            <div class="descriptions p-5">
                                <h4>Chinhung / Kinwa Lathe Machine</h4>
                                <div class="py-3"></div>
                                <span>PRODUCT DESCRIPTION</span>
                                <p>The high performance Lathe machines for your industrial purpose. We are the sole agent and distributor for Chinhung Kinwa Lathe.</p>
                                <div class="py-3"></div>
                                <span>Product Variants</span>
                                <p>CH430x1100, CH530x1100</p>
                                
                                <div class="clear"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="items">
                    <div class="row no-gutters">
                        <div class="col-md-30">
                            <div class="pictures back-white">
                                <img src="{{ app.request.baseurl }}/asset/images/machine_n1_big.jpg" alt="" class="img img-fluid d-block mx-auto">
                            </div>
                        </div>
                        <div class="col-md-30 back-grey my-auto text-left p-4">
                            <div class="descriptions p-5">
                                <h4>Chinhung / Kinwa Lathe Machine</h4>
                                <div class="py-3"></div>
                                <span>PRODUCT DESCRIPTION</span>
                                <p>The high performance Lathe machines for your industrial purpose. We are the sole agent and distributor for Chinhung Kinwa Lathe.</p>
                                <div class="py-3"></div>
                                <span>Product Variants</span>
                                <p>CH430x1100, CH530x1100</p>
                                
                                <div class="clear"></div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

            <div class="py-5"></div>
            <div class="py-2"></div>

            <div class="clear clearfix"></div>
        </div>
    </div>
</section>

{% endblock %}